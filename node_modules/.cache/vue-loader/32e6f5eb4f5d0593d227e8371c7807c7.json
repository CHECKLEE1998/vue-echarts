   emphasis: {\n              itemStyle: {\n                shadowBlur: 10,\n                shadowOffsetX: 0,\n                shadowColor: \"rgba(0, 0, 0, 0.5)\",\n              },\n            },\n          },\n        ],\n      });\n    },\n  },\n  mounted() {\n    this.$axios({\n      methods: \"get\",\n      url: \"https://edu.telking.com/api/?type=week\",\n    }).then((res) => {\n      const data = res.data.data;\n      for (let i = 0; i < data.xAxis.length; i++) {\n        this.seriesData.push({\n          name: data.xAxis[i],\n          value: data.series[i],\n        });\n      }\n      this.myPie();\n    });\n  },\n};\n</script>\n"]}]}