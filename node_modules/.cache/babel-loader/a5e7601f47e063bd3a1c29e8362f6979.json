    <nav class=\"nav\">\n      <div class=\"title\">\n        <h3>Audience Overview</h3>\n        <p>Roseville, CA</p>\n      </div>\n      <div class=\"menu\">\n        <ul>\n          <li\n            v-for=\"(item, index) in menuList\"\n            :key=\"index\"\n            @click=\"activeMenu(index)\"\n          >\n            {{ item.title }}\n          </li>\n        </ul>\n        <div class=\"linear\"></div>\n      </div>\n    </nav>\n\n    <div class=\"content\">\n      <!-- Rotation  -->\n      <div class=\"base-view\">\n        <div class=\"base-view-box\">\n          <Rotation />\n        </div>\n        <div class=\"base-view-box\">\n          <div class=\"top-title\">\n            <h4>Support Requests</h4>\n            <el-button>View All</el-button>\n          </div>\n          <div class=\"bottom-content\"></div>\n        </div>\n      </div>\n\n      <!-- Curve Chart -->\n      <div class=\"curve-chart\">\n        <Curve />\n      </div>\n\n      <!-- Statistical Graph -->\n      <div class=\"base-view\">\n        <div class=\"base-view-box\">\n          <Pie />\n        </div>\n        <div class=\"base-view-box\">\n          <Histogram />\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Rotation from \"@/components/Rotation\";\nimport { Curve, Pie, Histogram } from \"@/components/Echarts\";\nexport default {\n  name: \"Home\",\n  components: {\n    Rotation,\n    Curve,\n    Pie,\n    Histogram,\n  },\n  data() {\n    return {\n      menuList: [\n        {\n          title: \"DASHBOARD\",\n        },\n        {\n          title: \"PROJECTS\",\n        },\n        {\n          title: \"CALENDAR\",\n        },\n        {\n          title: \"MAILBOX\",\n        },\n        {\n          title: \"SETTINGS\",\n        },\n      ],\n    };\n  },\n  methods: {\n    activeMenu(index) {\n      let linear = document.querySelector(\".linear\");\n      let result = 100 * index;\n      linear.style.left = result + \"px\";\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n@import \"@/style/config.scss\";\n\n.home {\n  margin-top: 50px;\n  .nav {\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    .title {\n      h3 {\n        height: 23px;\n        font-family: Raleway-Regular;\n        font-size: 30px;\n        font-weight: normal;\n        color: #262b2e;\n      }\n      p {\n        height: 14px;\n        font-family: OpenSans;\n        font-size: 15px;\n        font-weight: normal;\n        color: #262b2e;\n        opacity: 0.7;\n      }\n    }\n    .menu {\n      position: relative;\n      ul {\n        display: flex;\n        li {\n          width: 100px;\n          font-weight: 600;\n          cursor: pointer;\n          font-size: 13px;\n          color: #262b2e;\n          text-align: center;\n        }\n      }\n      .linear {\n        position: absolute;\n        top: 20px;\n        left: 0;\n        width: 100px;\n        height: 2px;\n        background-color: #262b2e;\n        opacity: 0.2;\n        @include transItion(0.1s);\n      }\n    }\n  }\n\n  .content {\n    margin-top: 50px;\n    margin-bottom: 80px;\n    .base-view {\n      display: flex;\n      .base-view-box {\n        overflow: hidden;\n        // padding: 10px;\n        margin: 0 5px;\n        width: 560px;\n        height: 288px;\n        background-color: #fff;\n        .top-title {\n          height: 109px;\n          border-bottom: 1px solid #262b2e30;\n          display: flex;\n          justify-content: space-between;\n          align-items: center;\n          padding: 0 30px;\n        }\n      }\n    }\n    .curve-chart {\n      height: 350px;\n      margin: 20px 5px;\n      background-color: #fff;\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/views"}]}